{
    "name": "industrious/wp-helpers",
    "description": "A WP Helpers package.",
    "type": "library",
    "keywords": ["wordpress", "wp", "helpers"],
    "homepage": "https://github.com/industrious-mouse/wp-helpers",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Thomas",
            "email": "christian@industrious.agency",
            "homepage": "http://github.com/christian-thomas"
        }
    ],
    "support": {
        "source": "https://github.com/industrious-mouse/wp-helpers",
        "issues": "https://github.com/industrious-mouse/wp-helpers/issues"
    },
    "require": {
        "php": ">=5.5",
        "j7mbo/twitter-api-php": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Industrious\\WpHelpers\\": "src/"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Industrious\\Tests\\WpHelpers\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
